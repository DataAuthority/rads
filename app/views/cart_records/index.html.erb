<h1>Listing cart_records</h1>

<table>
  <thead>
    <tr>
      <th>Record</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cart_records.each do |cart_record| %>
      <tr>
        <td><%= cart_record.record_id %></td>
        <td><%= cart_record.user_id %></td>
        <td><%= link_to 'Destroy', cart_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
